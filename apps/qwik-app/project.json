{
  "name": "qwik-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/qwik-app/src",
  "targets": {
    "build": {
      "options": {
        "outputPath": "dist/apps/qwik-app"
      },
      "executor": "@nrwl/vite:build"
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "qwik-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "qwik-app:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "qwik-app:build:production",
          "hmr": false
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/qwik-app/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
